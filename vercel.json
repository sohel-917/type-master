{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/((?!assets/|favicon.svg|logo.png|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
